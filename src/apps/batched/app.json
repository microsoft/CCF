{
  "endpoints": {
    "/batch/submit": {
      "post": {
        "js_module": "batched.js",
        "js_function": "submit_batch",
        "forwarding_required": "always", "redirection_strategy": "to_primary",
        "authn_policies": ["user_cert"],
        "mode": "readwrite",
        "openapi": {}
      }
    },
    "/batch/fetch": {
      "post": {
        "js_module": "batched.js",
        "js_function": "fetch_batch",
        "forwarding_required": "sometimes", "redirection_strategy": "none",
        "authn_policies": ["user_cert"],
        "mode": "readonly",
        "openapi": {}
      }
    }
  }
}
