{
  "enclave": {
    "file": "libjs_generic.enclave.so.signed",
    "type": "release"
  },
  "network": {
    "node_to_node_interface": { "bind_address": "127.0.0.1:8081" },
    "rpc_interfaces": [{ "bind_address": "127.0.0.1:8080" }]
  },
  "command": {
    "type": "start",
    "network_certificate_file": "networkcert.pem",
    "start": {
      "constitution_files": [
        "validate.js",
        "apply.js",
        "resolve.js",
        "actions.js"
      ],
      "members": [
        {
          "certificate_file": "member0_cert.pem",
          "encryption_public_key_file": "member0_enc_pubk.pem"
        }
      ]
    }
  }
}
