  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "verbatimModuleSyntax": true,
    "allowJs": true,
    "lib": ["ES2020"],
    "declaration": true,
    "strict": true,
    "rootDir": "src",
    "outDir": "."
  },
  "include": ["src"],
  "exclude": []
}
